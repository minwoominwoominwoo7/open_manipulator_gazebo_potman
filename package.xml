<?xml version="1.0"?>
<package format="2">
  <name>open_manipulator_gazebo_potman</name>
  <version>1.1.0</version>
  <description>
    Gazebo configurations package for OpenManipulator
  </description>
  <maintainer email="test@test.todo">test</maintainer>
  <license>Apache 2.0</license>
  <author email="test@test.com">test</author>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>roscpp</depend>
  <depend>std_msgs</depend>
  <depend>gazebo_ros</depend>
  <depend>gazebo_ros_control</depend>
  <depend>controller_manager</depend>
  <depend>xacro</depend>
  <depend>urdf</depend>
  <export>
    <gazebo_ros plugin_path="${prefix}/lib" gazebo_media_path="${prefix}" gazebo_model_path="${prefix}/models"/>
  </export>
</package>
